<h1>Enter your contact information</h1>
<div>
  <div>
    <form action="/contacts" method="post">
      <label for="name">Name:</label>
      <input id="name" name="name" type="text">
      <label for="job">Job:</label>
      <input id="job" name="job" type="text">
      <label for="company">Company:</label>
      <input id="company" name="company" type="text">
      <br><br> 
      <label for="home">Home Phone:</label>
      <input id="home" name="home" type="text">
      <label for="work">Work Phone:</label>
      <input id="work" name="work" type="text">
  </div>
    <div>
      <h2>Home Address</h2>
      <label for="street">Street:</label>
      <input id="street" name="street" type="text">

      <label for="city">City:</label>
      <input id="city" name="city" type="text">

      <label for="state">State:</label>
      <input id="state" name="state" type="text">

      <label for="zip">Zip:</label>
      <input id="zip" name="zip" type="text">
    </div>
      <div>
        <h3>Work Address (optional)</h3>
        <label for="workStreet">Street:</label>
        <input id="workStreet" name="workStreet" type="text">

        <label for="workCity">City:</label>
        <input id="workCity" name="workCity" type="text">

        <label for="workState">State:</label>
        <input id="workState" name="workState" type="text">

        <label for="workZip">Zip:</label>
        <input id="workZip" name="workZip" type="text">
      </div>
      <button type="submit">Submit</button>
    </form>
</div>
